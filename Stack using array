#include <iostream>
using namespace std;

#define MAX 100

class Stack {
    int arr[MAX], top;
public:
    Stack() { top = -1; }

    void push(int x) {
        if (top == MAX-1) {
            cout << "Overflow\n";
            return;
        }
        arr[++top] = x;
    }

    void pop() {
        if (top == -1) {
            cout << "Underflow\n";
            return;
        }
        top--;
    }

    int peek() {
        return arr[top];
    }

    void display() {
        for (int i = 0; i <= top; i++)
            cout << arr[i] << " ";
        cout << endl;
    }
};

int main() {
    Stack s;
    s.push(10);
    s.push(20);
    s.push(30);
    s.display();

    s.pop();
    s.display();
}
